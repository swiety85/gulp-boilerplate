{
    "bitwise": true,
    //"camelcase": true, since bo serializer returns lowercase-snakecase mix we can not set this
    "curly": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef": false,
    "newcap": false,
    "noarg": true,
    "nonew": true,
    "quotmark": "single",
    "undef": true,
    "strict": true,
    "trailing": true,
    //"maxparams": 3, angular di needs more than 3 args
    "maxdepth": 2,
    "maxlen": 120,
    "es3": true,
    //"maxstatements": 10,
    "onevar": true,
    "node": true,
    "globals": {
        "angular": false,
        "breeze": false,
        "requirejs": false,
        "require": false,
        "define": false,
        "expect": false,
        "it": false,
        "spyOn": false,
        "describe": false,
        "beforeEach": false,
        "inject": false,
        "Class": false,
        "d3": false,
        "window": false
     }
}
